-- misc
function collide_car(player,car)
	return not (player.x>car.x+16
		or player.y>car.y+8
		or player.x+8<car.x
		or player.y+8<car.y)
end

function collide_love(player,love)
	return not (player.x>love.x+8
		or player.y>love.y+8
		or player.x+8<love.x
		or player.y+8<love.y)
end

-- love
function init_love()
	love={}
end

function spawn_love(e)
	local new_love={
		x=e.x,
		y=e.y;
		speed=speed/2,
		type=63
	}
	add(love, new_love)
end

function update_love()
	for l in all(love) do
		l.y-=l.speed+boost
		if l.y<c_min_screen-8 then
			del(love,l)
		end
	end
end

function draw_love()
	for l in all(love) do
		spr(l.type,l.x,l.y)
	end
end

-- building
function init_building()
	map_y=0
end

function update_building()
	map_y-=speed+boost
	if map_y<-127 then map_y=0 end
end

function draw_building()
	map(0,0,0,map_y,16,16)
	map(0,0,0,128+map_y,16,16)
end

function draw_building_background()
	cls(12)
	-- sun
	circfill(27, 27, 8, 10)
	-- clouds left
	line(18, 40, 30, 40, 7)
	line(20, 45, 35, 45, 7)
	line(17, 50, 23, 50, 7)
	line(28, 55, 35, 55, 7)
	-- clouds rigth
	line(90, 25, 103, 25, 7)
	line(95, 35, 100, 35, 7)
	line(89, 40, 95, 40, 7)
	line(90, 50, 105, 50, 7)
	line(92, 55, 95, 55, 7)
	line(90, 60, 110, 60, 7)
end

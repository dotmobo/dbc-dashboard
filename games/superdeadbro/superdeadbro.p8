-- bird
function init_birds()
	birds={}
end

function update_birds()
	-- spawn birds
	if #birds==0 then
		for i=0,rnd(8) do
			local b={x=c_max_screen+rnd(64),y=64+rnd(80),s=rnd({25,41,57}),a=0}
				add(birds,b)
		end
	end
	-- move birds
	for b in all(birds) do
		b.x-=speed/2
		b.y-=speed/2+boost
		--del bird outside screen
		if b.x<c_min_screen-4 or b.y<c_min_screen-4 then
			del(birds,b)
		end
		-- animations
		b.a+=1
		if b.a%12==0 then
			b.s+=1
		end
		if b.s==29 then
			b.s=25
			b.a=0
		end
		if b.s==45 then
			b.s=41
			b.a=0
		end
		if b.s==61 then
			b.s=57
			b.a=0
		end
	end
end

function draw_birds()
	for b in all(birds) do
		spr(b.s,b.x,b.y,0.5,0.5)
	end
end

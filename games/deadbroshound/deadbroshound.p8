--explosions
function create_explosions(x,y)
    return {
        items={},
        update=function(self)
            for e in all(self.items) do
                e.timer+=1
                if e.timer==13 then
                    del(self.items,e)
                end
            end
        end,
        draw=function(self)
            for e in all(self.items) do
                circ(e.x,e.y,e.timer/3,
                8+e.timer%3)
            end
        end,
        add=function(self,x,y)
            add(self.items,{x=x,y=y,timer=0})
        end
    }
end
